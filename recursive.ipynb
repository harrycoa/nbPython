   "source": [
    "def fact(n: int) -> int:\n",
    "    if n > 1:\n",
    "        return n * fact(n-1)\n",
    "    else:\n",
    "        return 1\n",
    "\n",
    "print('6! =', fact(6))\n",
    "print('6! =', fact(0))"
   ]
  }
